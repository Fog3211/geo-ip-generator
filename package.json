{
  "name": "geo-ip-generator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "data:import:ip2location": "tsx scripts/import-ip2location.ts",
    "data:import:territories": "tsx scripts/import-world-territories.ts",
    "data:export:combined": "tsx scripts/generate-combined-data.ts",
    "setup": "prisma migrate dev && pnpm run data:import:territories && pnpm run data:import:ip2location",
    "setup:json": "pnpm run data:export:combined",
    "data:sync": "tsx scripts/sync-data.ts",
    "data:export:csv": "tsx scripts/export-csv.ts",
    "data:export:excel": "tsx scripts/export-excel.ts",
    "data:validate": "tsx scripts/validate-ip-data.ts",
    "data:validate:sample": "tsx scripts/validate-ip-data.ts 50",
    "postinstall": "prisma generate",
    "predev": "tsx scripts/prestart.ts",
    "prestart": "tsx scripts/prestart.ts",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/csv-stringify": "^3.1.3",
    "csv-parse": "^6.0.0",
    "csv-stringify": "^6.6.0",
    "exceljs": "^4.4.0",
    "ioredis": "^5.6.1",
    "next": "^15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "postcss": "^8.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}
